<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SupplyPart.
    -->
    <changeSet id="20180405084055-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="contract_id"
                                 baseTableName="supply_part"
                                 constraintName="fk_supply_part_contract_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supply_part_contract"/>

        <addForeignKeyConstraint baseColumnNames="bom_id"
                                 baseTableName="supply_part"
                                 constraintName="fk_supply_part_bom_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bill_of_materials"/>

    </changeSet>
</databaseChangeLog>
